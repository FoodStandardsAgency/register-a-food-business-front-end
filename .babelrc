{
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  },
  "plugins": ["@babel/plugin-transform-runtime"],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "12",
          "edge": "16",
          "firefox": "60",
          "chrome": "67",
          "safari": "10",
          "ie": "11"
        },
        "corejs": {
          "version": "3.1"
        },
        "useBuiltIns": "entry"
      }
    ],
    "next/babel"
  ]
}
