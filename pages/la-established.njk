{% from "govuk/components/fieldset/macro.njk" import govukFieldset %}
{% from "govuk/components/button/macro.njk" import govukButton %}
{% from "../components/PostForm.njk" import postForm %}
{% extends "layout.njk" %}
{% block content %}
  {{ props.currentPageWithQuery }}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      {% call postForm(props.formAction, props.csrfToken, props.language) -%}
      {% call govukFieldset({
        legend: {
          text: __("You are registering with "+ props.cumulativeFullAnswers.local_authority_name , props.language),
          
          classes: "govuk-fieldset__legend--l",
          isPageHeading: true
        }
        }) %}

      <p class="govuk-body">
        {% endcall %}
        {{ govukButton({
        text:  __("Continue", props.language)
      }) }}
        {%- endcall %}
      </div>
    </div>
  {% endblock %}