{
  "name": "register_a_food_business_front_end",
  "version": "18.0.0",
  "engines": {
    "node": ">=22.14.0"
  },
  "module": "es2022",
  "scripts": {
    "dev": "npm run sass-dev && node ./src/server/server.js",
    "start": "NODE_ENV=production node ./src/server/server.js",
    "debug": "npm run sass-dev && node --inspect src/server/server.js",
    "debug:prod": "npm run sass-dev && NODE_ENV=production node --inspect src/server/server.js",
    "local:dev": "npm run sass-dev && nodemon ./src/server/server.js",
    "local:start": "npm run sass-dev && (export $(grep -v '^#' .env | xargs -d '\n')) && cross-env NODE_ENV=production nodemon ./src/server.js",
    "local:debug": "npm run sass-dev && nodemon --inspect src/server/server.js",
    "local:debug:prod": "npm run sass-dev && NODE_ENV=production nodemon --inspect src/server/server.js",
    "version": "NODE_ENV=production node -v",
    "testindex": "jest index",
    "test": "jest ./src ./pages --coverage",
    "test:ci": "export TEST_TYPE=unit && jest ./src ./pages --coverage --maxWorkers=4",
    "test:watch": "jest ./src --watch",
    "test:debug": "node --inspect=127.0.0.1:9225 node_modules/.bin/jest --runInBand",
    "test:securityallWizard": "snyk wizard",
    "test:securityWizard": "snyk wizard --severity-threshold=medium",
    "test:security": "snyk auth && snyk test --severity-threshold=medium",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:verify": "prettier --list-different src/ docs/ '!./src/__tests__/__snapshots__/**/*' '!./**/*.pdf'",
    "format": "prettier --write './src/**/*' './public/static/locales/**/*' './docs/**/*' '!./**/*.pdf' '!./src/__tests__/__snapshots__/**/*'",
    "jsdoc": "rm -rf ./docs/jsdoc && jsdoc -r ./src -d ./docs/jsdoc",
    "prepublish": "browserify src/server/static/auto-complete/BusinessTypeLookupFunctions.js src/server/static/auto-complete/main.js > src/server/static/auto-complete/bundle.js",
    "sass-dev": "sass --update --style=expanded src/server/css:src/server/css",
    "sass-dev-watch": "sass --watch --update --style=expanded src/server/css:src/server/css",
    "sass-prod": "sass --no-source-map --style=compressed src/server/css:src/server/css"
  },
  "dependencies": {
    "@ons/design-system": "^72.11.0",
    "@slice-and-dice/register-a-food-business-validation": "^1.30.1",
    "accessible-autocomplete": "3.0.1",
    "applicationinsights": "2.9.6",
    "applicationinsights-native-metrics": "^0.0.11",
    "axios": "^1.8.4",
    "browserify": "^17.0.1",
    "cheerio": "1.0.0-rc.12",
    "cls-hooked": "^4.2.2",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "csurf": "^1.2.2",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-useragent": "^1.0.15",
    "fuse.js": "^7.1.0",
    "get-random-values": "^3.0.0",
    "govuk-frontend": "^5.9.0",
    "helmet": "^8.1.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "i18n": "^0.15.1",
    "jsonschema": "^1.5.0",
    "moment": "^2.30.1",
    "mongodb": "^6.16.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.10",
    "nunjucks": "^3.2.4",
    "nunjucks-date-filter": "^0.1.1",
    "prop-types": "^15.8.1",
    "stem-porter": "^0.0.1",
    "uuid": "11.1.0",
    "validator": "^13.15.0",
    "winston": "^3.17.0",
    "winston-azure-application-insights": "^4.0.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "browserify": "^17.0.1",
    "eslint": "^9.25.1",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-nunjucks": "^1.0.0",
    "jsdoc": "^4.0.4",
    "prettier": "^3.5.3",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.87.0",
    "snyk": "^1.1296.2",
    "source-map-resolve": "^0.6.0"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "printWidth": 100,
    "semi": true
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "node_modules",
      ".next"
    ],
    "watch": [
      "pages",
      "src"
    ],
    "ext": "js json njk"
  }
}
