{
  "name": "register_a_food_business_front_end",
  "version": "8.4.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./src/server/server.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node ./src/server/server.js",
    "start:local": "(export $(grep -v '^#' .env | xargs -d '\n')) && cross-env NODE_ENV=production node ./src/server.js",
    "debug": "node --inspect src/server/index.js",
    "debug:prod": "NODE_ENV=production node --inspect src/server/index.js",
    "debug:prod:local": "(export $(grep -v '^#' .env | xargs -d '\n')) && NODE_ENV=production node --inspect src/server/index.js",
    "version": "NODE_ENV=production node -v",
    "test": "jest ./src ./pages --coverage",
    "test:ci": "export TEST_TYPE=unit && jest ./src ./pages --coverage --maxWorkers=4",
    "test:watch": "jest ./src --watch",
    "test:integration": "export TEST_TYPE=integration && jest tests/integration/*.js",
    "test:contract": "jest tests/contract/*.js",
    "test:securityallWizard": "snyk wizard",
    "test:securityWizard": "snyk wizard --severity-threshold=medium",
    "test:security": "snyk auth && snyk test --severity-threshold=medium",
    "lint": "eslint ./src ./pages",
    "lint:fix": "eslint --fix ./src ./pages",
    "format:verify": "prettier --list-different src/ pages docs/ tests/  '!./src/__tests__/__snapshots__/**/*' '!./**/*.pdf'",
    "format": "prettier --write src/ pages docs/ tests/ '!./**/*.pdf' '!./src/__tests__/__snapshots__/**/*'",
    "jsdoc": "rm -rf ./docs/jsdoc && jsdoc -r ./src -d ./docs/jsdoc",
    "updateBusinessTypes": "node ./src/components/update-business-types.js && yarn format"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@govuk-react/icon-crown": "^0.0.5",
    "@govuk-react/icons": "^0.7.1",
    "@slice-and-dice/register-a-food-business-validation": "^1.17.0",
    "accessible-autocomplete": "^2.0.2",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.17.0",
    "express-useragent": "^1.0.13",
    "govuk-react": "^0.7.2",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.0",
    "jest-junit": "^10.0.0",
    "jsonschema": "^1.2.6",
    "mongodb": "^3.5.6",
    "next": "^9.3.5",
    "node-cache": "^5.1.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hooks": "^1.0.1",
    "react-is": "^16.13.1",
    "react-markdown": "^4.3.1",
    "stemmer": "^1.0.5",
    "styled-components": "4",
    "validator": "^13.0.0",
    "winston": "2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@storybook/addons": "^5.3.18",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "jest": "^24.9.0",
    "jest-emotion": "^9.2.11",
    "jest-next-dynamic": "^1.0.1",
    "jsdoc": "^3.6.4",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "source-map-resolve": "^0.6.0",
    "synk": "^0.0.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "dynamic-import-node"
        ]
      }
    },
    "plugins": [
      [
        "emotion",
        {
          "inline": true
        }
      ],
      "inline-react-svg"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "edge": "16",
            "firefox": "60",
            "chrome": "67",
            "safari": "10",
            "ie": "11"
          },
          "corejs": {
            "version": "3.1"
          },
          "useBuiltIns": "entry"
        }
      ],
      "next/babel"
    ]
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "emotion/jsx-import": "error",
      "emotion/no-vanilla": "error",
      "emotion/import-from-emotion": "error",
      "emotion/styled-import": "error",
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": "warn",
      "no-param-reassign": [
        2,
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "req"
          ]
        }
      ]
    },
    "plugins": [
      "emotion",
      "prettier"
    ]
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "node_modules",
      ".next"
    ],
    "watch": [
      "pages",
      "src"
    ],
    "ext": "js json"
  }
}
