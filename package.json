{
  "name": "register_a_food_business_front_end",
  "version": "13.1.0",
  "engines": {
    "node": ">=12.9"
  },
  "scripts": {
    "start-temp-njk-test-server": "node ./src/server/exp-njk-server.js",
    "dev": "node ./src/server/server.js",
    "start": "NODE_ENV=production node ./src/server/server.js",
    "debug": "node --inspect src/server/server.js",
    "debug:prod": "NODE_ENV=production node --inspect src/server/server.js",
    "local:dev": "nodemon ./src/server/server.js",
    "local:start": "(export $(grep -v '^#' .env | xargs -d '\n')) && cross-env NODE_ENV=production nodemon ./src/server.js",
    "local:debug": "nodemon --inspect src/server/server.js",
    "local:debug:prod": "NODE_ENV=production nodemon --inspect src/server/server.js",
    "version": "NODE_ENV=production node -v",
    "testindex": "jest index",
    "test": "jest ./src ./pages --coverage",
    "test:ci": "export TEST_TYPE=unit && jest ./src ./pages --coverage --maxWorkers=4",
    "test:watch": "jest ./src --watch",
    "test:debug": "node --inspect=127.0.0.1:9225 node_modules/.bin/jest --runInBand",
    "test:integration": "export TEST_TYPE=integration && jest tests/integration/*.js",
    "test:contract": "jest tests/contract/*.js",
    "test:securityallWizard": "snyk wizard",
    "test:securityWizard": "snyk wizard --severity-threshold=medium",
    "test:security": "snyk auth && snyk test --severity-threshold=medium",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src ./pages",
    "format:verify": "prettier --list-different src/ docs/ tests/ '!./src/__tests__/__snapshots__/**/*' '!./**/*.pdf'",
    "format": "prettier --write './src/**/*' './public/static/locales/**/*' './docs/**/*' './tests/**/*' '!./**/*.pdf' '!./src/__tests__/__snapshots__/**/*'",
    "jsdoc": "rm -rf ./docs/jsdoc && jsdoc -r ./src -d ./docs/jsdoc",
    "updateBusinessTypes": "node ./src/components/update-business-types.js && yarn format",
    "prepublish": "browserify src/server/static/auto-complete/BusinessTypeLookupFunctions.js src/server/static/auto-complete/main.js > src/server/static/auto-complete/bundle.js"
  },
  "resolutions": {
    "styled-components": "^5.1.0"
  },
  "dependencies": {
    "@ons/design-system": "^44.1.2",
    "@slice-and-dice/register-a-food-business-validation": "1.21.6",
    "accessible-autocomplete": "^2.0.3",
    "applicationinsights": "^1.7.6",
    "applicationinsights-native-metrics": "^0.0.5",
    "browserify": "^17.0.0",
    "cheerio": "^1.0.0-rc.6",
    "cls-hooked": "^4.2.2",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.17.0",
    "express-useragent": "^1.0.13",
    "fuse.js": "^6.4.6",
    "get-random-values": "^1.2.2",
    "govuk-frontend": "^3.11.0",
    "helmet": "^3.22.1",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.0",
    "i18n": "^0.13.2",
    "i18next": "^20.2.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "i18next-http-middleware": "^3.1.1",
    "jest-junit": "^10.0.0",
    "jsonschema": "^1.2.6",
    "moment": "2.24.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "mongodb": "^3.6.6",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^2.0.3",
    "nunjucks": "^3.2.3",
    "nunjucks-date-filter": "^0.1.1",
    "prop-types": "^15.7.2",
    "sass": "^1.32.11",
    "stem-porter": "0.0.1",
    "styled-components": "^5.1.0",
    "uuid": "^7.0.3",
    "validator": "^13.0.0",
    "winston": "^3.2.1",
    "winston-azure-application-insights": "^2.0.0",
    "winston-elasticsearch": "^0.8.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addons": "^5.3.18",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "browserify": "^17.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "jest": "^24.9.0",
    "jest-axe": "^4.1.0",
    "jest-emotion": "^9.2.11",
    "jest-next-dynamic": "^1.0.1",
    "jest-nunjucks": "^1.0.0",
    "jsdoc": "^3.6.4",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "source-map-resolve": "^0.6.0",
    "synk": "^0.0.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "dynamic-import-node"
        ]
      }
    },
    "plugins": [
      "babel-plugin-styled-components",
      "@babel/plugin-transform-runtime",
      "inline-react-svg"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "12",
            "edge": "16",
            "firefox": "60",
            "chrome": "67",
            "safari": "10",
            "ie": "11"
          },
          "corejs": {
            "version": "3.1"
          },
          "useBuiltIns": "entry"
        }
      ],
      "@babel/preset-react",
      "next/babel"
    ]
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "emotion/jsx-import": "error",
      "emotion/no-vanilla": "error",
      "emotion/import-from-emotion": "error",
      "emotion/styled-import": "error",
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": "warn",
      "no-param-reassign": [
        2,
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "req"
          ]
        }
      ]
    },
    "plugins": [
      "emotion",
      "prettier"
    ]
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "node_modules",
      ".next"
    ],
    "watch": [
      "pages",
      "src"
    ],
    "ext": "js json"
  },
  "jest": {
    "transform": {
      "^.+\\.njk$": "jest-nunjucks"
    }
  }
}
