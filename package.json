{
  "name": "register_a_food_business_front_end",
  "version": "7.0.0",
  "scripts": {
    "debug": "node --inspect src/server/index.js",
    "debug:prod": "NODE_ENV=production node --inspect src/server/index.js",
    "dev": "node src/server/index.js",
    "build": "next build",
    "version": "NODE_ENV=production node -v",
    "start": "NODE_ENV=production node src/server/index.js",
    "test": "jest ./src ./pages --coverage",
    "test:ci": "export TEST_TYPE=unit && jest ./src ./pages --coverage --maxWorkers=4",
    "test:watch": "jest ./src --watch",
    "test:integration": "export TEST_TYPE=integration && jest tests/integration/*.js",
    "test:contract": "jest tests/contract/*.js",
    "test:securityallWizard": "snyk wizard",
    "test:securityWizard": "snyk wizard --severity-threshold=medium",
    "test:security": "snyk auth && snyk test --severity-threshold=medium",
    "lint": "eslint ./src ./pages",
    "format:verify": "prettier --list-different '{./src,./pages,./docs,./tests}/**/*' '!./src/__tests__/__snapshots__/**/*'",
    "format": "prettier --write '{./src,./pages,./docs,./tests}/**/*' '!./src/__tests__/__snapshots__/**/*'",
    "jsdoc": "rm -rf ./docs/jsdoc && jsdoc -r ./src -d ./docs/jsdoc",
    "updateBusinessTypes": "node ./src/components/update-business-types.js && npm run format"
  },
  "dependencies": {
    "@govuk-react/icon-crown": "0.0.5",
    "@govuk-react/icons": "0.2.7",
    "@slice-and-dice/register-a-food-business-validation": "1.14.3",
    "accessible-autocomplete": "1.6.1",
    "body-parser": "1.18.3",
    "connect-mongo": "2.0.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "6.0.0",
    "dotenv-webpack": "^1.5.7",
    "emotion-server": "9.2.8",
    "express": "4.16.3",
    "express-rate-limit": "3.2.0",
    "express-session": "1.15.6",
    "govuk-react": "0.3.1",
    "jsonschema": "1.2.4",
    "mongodb": "3.1.6",
    "next": "6.1.1",
    "node-cache": "4.2.0",
    "node-fetch": "2.1.2",
    "react-emotion": "9.2.8",
    "stemmer": "1.0.2",
    "validator": "10.3.0",
    "winston": "2.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-rc.1",
    "@babel/polyfill": "7.0.0-rc.1",
    "@babel/preset-env": "7.0.0-rc.1",
    "@storybook/addons": "3.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "23.4.2",
    "babel-plugin-emotion": "9.1.2",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "emotion": "9.2.8",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.4.0",
    "eslint": "4.19.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.8.2",
    "jest": "24.8.0",
    "jest-emotion": "9.2.7",
    "jest-junit": "5.2.0",
    "jsdoc": "3.5.5",
    "prettier": "1.12.1",
    "prop-types": "15.6.1",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-markdown": "3.3.2",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "16.4.2",
    "snyk": "^1.94.0",
    "webpack": "3.1.0"
  },
  "license": "MIT"
}
